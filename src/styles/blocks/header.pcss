.header{
    overflow: hidden;
    display: grid;
    grid-template-columns: 15% 65% 20%;
    margin-top: 64px;
    z-index: 10;
    @include tablets{
        grid-template-columns: 60% 40%;
    }
    @include phones{
        grid-template-columns: 70% 40%;
        .links{
            display: none;
        }
    }
}